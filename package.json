{
  "name": "@artsy/cohesion",
  "version": "0.0.2",
  "description": "Analytics schema and library helpers",
  "main": "dist/index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artsy/cohesion.git"
  },
  "author": "",
  "license": "MIT",
  "scripts": {
    "compile": "babel src --out-dir dist --extensions '.ts' --ignore src/**/__tests__",
    "test": "jest",
    "type-check": "tsc --noEmit --pretty"
  },
  "devDependencies": {
    "@artsy/auto-config": "^1.0.2",
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-typescript": "7.9.0",
    "@types/jest": "25.2.1",
    "babel-jest": "25.3.0",
    "babel-plugin-module-resolver": "4.0.0",
    "husky": "4.2.5",
    "jest": "25.3.0",
    "jest-junit": "10.0.0",
    "lint-staged": "10.1.3",
    "prettier": "2.0.4",
    "tslint": "6.1.1",
    "typescript": "3.8.3"
  },
  "dependencies": {},
  "jest": {
    "transform": {
      ".ts": "babel-jest"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts)": [
      "yarn run prettier --write"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": true
  },
  "bugs": {
    "url": "https://github.com/artsy/cohesion/issues"
  },
  "homepage": "https://github.com/artsy/cohesion#readme"
}
